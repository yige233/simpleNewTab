{
  "name": "__MSG_app_name__",
  "version": "2.0.3",
  "description": "__MSG_app_desc__",
  "permissions": ["commands", "alarms"],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "96": "icon/icon96.png",
    "128": "icon/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "html/newtab.html"
  },
  "action": {},
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "commands": {
    "newNote": {
      "description": "__MSG_app_newNote__"
    },
    "saveBgPos": {
      "description": "__MSG_app_saveBgPos__",
      "suggested_key": {
        "default": "Alt+S"
      }
    },
    "delBgPos": {
      "description": "__MSG_app_delBgPos__",
      "suggested_key": {
        "default": "Alt+D"
      }
    }
  },
  "options_page": "html/options.html",
  "author": "__MSG_app_author__",
  "default_locale": "zh_CN",
  "manifest_version": 3,
  "homepage_url": "https://github.com/yige233/simpleNewTab"
}
